{
  "version": 3,
  "configurePresets": [
    {
      "name": "x64-debug",
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "installDir": "${sourceDir}/out/install/${presetName}",
      "generator": "Ninja",
      "architecture": {
        "strategy": "external",
        "value": "x64"
      },
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "x64-release",
      "inherits": "x64-debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "x64-debug",
      "configurePreset": "x64-debug",
      "configuration": "Debug",
      "verbose": true
    },
    {
      "name": "x64-release",
      "configurePreset": "x64-release",
      "configuration": "Release",
      "verbose": true
    }
  ]
}